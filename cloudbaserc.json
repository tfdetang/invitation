{
  "$schema": "https://framework-1258016615.tcloudbaseapp.com/schema/latest.json",
  "envId": "{{env.ENV_ID}}",
  "framework": {
    "name": "react-invitation",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "buildCommand": "npm run build",
          "outputPath": "dist",
          "cloudPath": "/",
          "ignore": [
            ".git",
            ".github",
            "node_modules",
            ".DS_Store"
          ]
        }
      }
    }
  }
}
